<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="cycronix.CTandroid"
      android:versionCode="1"
      android:versionName="0.1">
    <application android:icon="@drawable/appengine" android:label="CTserver">
        <activity android:name="cycronix.CTandroid.CTAserver"
                  android:label="CTserver"
                  android:configChanges="keyboardHidden|orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            android:debuggable="true"
        </activity> 
    </application>
 
    <uses-permission android:name="android.permission.INTERNET" />  
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />  
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />  
    
     
<uses-sdk android:minSdkVersion="19"></uses-sdk>
</manifest> 